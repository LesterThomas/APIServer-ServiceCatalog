{
    "extends": "strongloop",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
      },    
    "rules": {
        "max-len": 0,         // specify the maximum length of a line in your program (off by default)
        "strict": 0
    },
    "env": {
        "es6": true
    }
}
